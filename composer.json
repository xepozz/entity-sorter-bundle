{
  "name": "xepozz/entity-sorter-bundle",
  "description": "Adds sorting functionality to entities",
  "keywords": [
    "sort",
    "sorter-bundle",
    "entity-sorter",
    "entity-sorter-bundle",
    "symfony"
  ],
  "type": "symfony-bundle",
  "license": "MIT",
  "archive": {
    "exclude": [
      "/"
    ]
  },
  "require": {
    "php": ">=5.6",
    "symfony/framework-bundle": "^4.4|~5.0",
    "doctrine/orm": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Xepozz\\EntitySorterBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xepozz\\EntitySorterBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6 "
  }
}
